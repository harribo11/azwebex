{
    "name": "azure-justrun",
    "version": "v0.1.8",
    "types": {
        "azure-justrun:index:PublicAccess":{
            "type": "string",
            "description": "Duplicates azure-native:storage:PublicAccess",
            "enum": [
                {
                    "value": "Container"
                },
                {
                    "value": "Blob"
                },
                {
                    "value": "None"
                }
            ]
        },
        "azure-justrun:index:SkuName":{
            "type": "string",
            "description": "Duplicates azure-native:storage:SkuName",
            "enum": [
                {
                    "value": "Standard_LRS"
                },
                {
                    "value": "Standard_GRS"
                },
                {
                    "value": "Standard_RAGRS"
                },
                {
                    "value": "Standard_ZRS"
                },
                {
                    "value": "Premium_LRS"
                },
                {
                    "value": "Premium_ZRS"
                },
                {
                    "value": "Standard_GZRS"
                },
                {
                    "value": "Standard_RAGZRS"
                }
            ]
        }

    },
    "resources": {
        "azure-justrun:index:webapp":{
            "isComponent": true,
            "inputProperties": {
                "appSkuName": {
                    "type": "string",
                    "description": "The name of the compute instance running the server. Also see appSkuTier",
                    "default": "B1"
                },
                "appSkuTier": {
                    "type": "string",
                    "description": "The tier of the compute instance running the server. Also see appSkuName",
                    "default": "Basic"
                },
                "filePath": {
                    "type": "string",
                    "description": "The relative file path to the folder containing web files.",
                    "default": "wwwroot"
                },
                "containerPublicAccess": {
                    "$ref": "#/types/azure-justrun:index:PublicAccess",
                    "description": "The public access level of the BlobContainer containg the website data."
                },
                "storageSkuName":{
                    "$ref": "#/types/azure-justrun:index:SkuName",
                    "description": "The name of the SKU of the storage account created, if storageAccount is not provided"
                },
                "storageAccount":{
                    "$ref": "/azure-native/v1.64.1/schema.json#/resources/azure-native:storage:StorageAccount",
                    "description": "The storage account to use. One will be created if not provided."
                },
                "resourceGroup": {
                    "$ref": "/azure-native/v1.64.1/schema.json#/resources/azure-native:resources:ResourceGroup",
                    "description": "The resource group to use. One will be created if not provided."
                },
                "namePrefix": {
                    "type": "string",
                    "description": "The name prefix given to child resources of this component. Should not contain dashes."
                }
            },
            "properties": {
                "url": {
                    "type": "string"
                }
            }
        }
    },
    "language": {
        "csharp": {
            "packageReferences": {
                "Pulumi": "3.*",
                "Pulumi.AzureNative": "1.*"
            },
            "respectSchemaVersion": true
        },
        "go": {
            "generateResourceContainerTypes": true,
            "importBasePath": "github.com/pulumi/pulumi-azure-justrun/sdk/go/azure-justrun",
            "respectSchemaVersion": true
        },
        "nodejs": {
            "dependencies": {
                "@pulumi/pulumi": "^3.0.0",
                "@pulumi/azure-native": "^1.0.0"
            },
            "respectSchemaVersion": true,
            "containsEnums": true
        },
        "python": {
            "requires": {
                "pulumi": ">=3.0.0,<4.0.0",
                "pulumi-azure-native": ">=1.0.0,<2.0.0"
            },
            "respectSchemaVersion": true
        }
    }
}